<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="product_form">
            <block after="-" class="Webkul\Walletsystem\Block\Adminhtml\ProductSetAttribute" name="tmp_walletsystem_script">
            	<arguments name="wallet_cash_back">
                    <argument name="config" xsi:type="array">
                        <item name="collapsible" xsi:type="boolean">false</item>
                        <item name="label" xsi:type="string" translate="true" />
                        <item name="sortOrder" xsi:type="string">2000</item>
                        <item name="canShow" xsi:type="boolean">true</item>
                        <item name="validation" xsi:type="array">
                            <item name="required-entry" xsi:type="string">true</item>
                        </item>                   
                    </argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>