<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
<acl>
    <resources>
      
        <resource id="Magento_Backend::admin">
              <!-- Menu of Extension -->
            <resource id="Milople_All::all" title="Milople Extensions" sortOrder="10" >
                <resource id="Milople_Personalizedcool::personalized" title="Milople Personalized" sortOrder="40">
					<resource id="Milople_Personalizedcool::config" title="Personalized Configuration" sortOrder="40">
					</resource>
					<resource id="Milople_Personalizedcool::fonts" title="Personalized Fonts" sortOrder="40">
					</resource>
					<resource id="Milople_Personalizedcool::clip_category" title="Personalized Category" sortOrder="40">
					</resource>
					<resource id="Milople_Personalizedcool::clip_image" title="Personalized Clipart Image" sortOrder="40">
					</resource>
				</resource>
			</resource>
             <!-- System Config Section of Extension -->
            <resource id="Magento_Backend::stores">
                <resource id="Magento_Backend::stores_settings">
                    <resource id="Magento_Config::config">
                        <resource id="Milople_Personalizedcool::config_personalized" title="Milople System Config Section" />
                    </resource>
                </resource>
            </resource>
        </resource>
    </resources>
</acl>
</config>